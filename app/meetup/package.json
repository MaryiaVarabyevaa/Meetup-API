{
  "name": "my-express-app",
  "version": "1.0.0",
  "description": "An Express app using TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "start:dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only ./src/index.ts",
    "start": "cross-env NODE_ENV=production ts-node-dev --respawn --transpile-only ./src/index.ts",
    "build": "tsc",
    "db:create:dev": "NODE_ENV=development ts-node ./src/db/createDB.ts",
    "db:create": "NODE_ENV=production ts-node ./src/db/createDB.ts",
    "db:migrate:dev": "NODE_ENV=development npx prisma migrate dev --name init --schema=./src/db/prisma.schema",
    "db:migrate": "NODE_ENV=production npx prisma migrate dev --name init --schema=./src/db/prisma.schema",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier --write ./src/**/*.ts",
    "prettier:fix": "echo fixing Prettier formatting"
  },
  "keywords": [
    "express",
    "typescript"
  ],
  "author": "Your Name",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.13.0",
    "@types/sequelize": "^4.28.14",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "dotenv-flow": "^3.2.0",
    "express": "^4.17.1",
    "pdfkit": "^0.13.0",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/pdfkit": "^0.12.9",
    "@types/pg": "^8.6.6",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript-prettier": "^5.0.0",
    "eslint-plugin-import": "^2.27.5",
    "prettier": "^2.8.8",
    "prisma": "^4.13.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.3.5"
  }
}
